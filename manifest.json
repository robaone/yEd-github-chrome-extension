{
    "name": "yEd-github viewer",
    "version": "1.0.1",
    "description": "Add a viewer button to Github for viewing .graphml files in the yEd-live editor.",
    "permissions": ["tabs","declarativeContent","scripting"],
    "host_permissions": ["https://github.com/*/blob/*.graphml"],
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/yEd-github16.png",
        "32": "images/yEd-github32.png",
        "48": "images/yEd-github48.png",
        "128": "images/yEd-github128.png"
      }
    },
    "icons": {
      "16": "images/yEd-github16.png",
      "32": "images/yEd-github32.png",
      "48": "images/yEd-github48.png",
      "128": "images/yEd-github128.png"
    },
    "manifest_version": 3
}
